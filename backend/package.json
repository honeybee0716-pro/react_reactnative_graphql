{
  "name": "improve",
  "version": "1.0.0",
  "description": "Improve",
  "main": "./dist/src/index.js",
  "repository": "https://github.com/joeyfenny/improve",
  "author": "Joey Fenny",
  "license": "MIT",
  "heroku-run-build-script": true,
  "engines": {
    "node": "16.15.1"
  },
  "scripts": {
    "dev": "yarn prisma db push && tsc && nodemon -r dotenv/config --watch 'src/**/*.ts' --exec \"ts-node\" src/index.ts dotenv_config_path=./.env",
    "build": "npx prisma db push && tsc",
    "test": "jest --runInBand --detectOpenHandles",
    "prettier": "prettier --write \"**/*.{tsx,ts,css,md,js,html}\"",
    "eslint": "eslint ./src/**/*.ts --fix",
    "lint": "eslint .",
    "postinstall": "yarn build"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "prettier --write",
      "eslint"
    ],
    "*.{md,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@graphql-tools/merge": "^8.2.10",
    "@graphql-tools/schema": "^8.3.2",
    "@prisma/client": "^3.15.0",
    "@sendgrid/mail": "^7.6.2",
    "@sentry/node": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "@slack/webhook": "^6.1.0",
    "@types/dotenv": "^8.2.0",
    "apollo-server": "^3.3.0",
    "apollo-server-express": "^3.9.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "bson": "^4.6.4",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "global": "^4.4.0",
    "graphql": "^15.5.0",
    "graphql-middleware": "^6.1.15",
    "graphql-shield": "^7.5.0",
    "graphql-tools": "^8.2.0",
    "graphql-type-json": "^0.3.2",
    "jest": "^27.0.4",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.4",
    "mongoose": "^5.12.15",
    "node-fetch": "^3.2.6",
    "nodemon": "^2.0.7",
    "redis": "^4.1.0",
    "stripe": "^8.220.0",
    "ts-node": "^10.0.0",
    "tsc": "^2.0.3",
    "tslint": "^6.1.3",
    "twilio": "^3.77.0",
    "universal-cookie": "^4.0.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/faker": "^5.5.7",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.0",
    "@types/rosie": "^0.0.39",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "faker": "^5.5.3",
    "jscpd": "^3.4.5",
    "lint-staged": "^10.5.1",
    "mock-jwks": "^1.0.1",
    "nock": "^13.1.3",
    "prettier": "^2.3.1",
    "prisma": "^3.15.0",
    "rosie": "^2.1.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4"
  }
}
